{
  "name": "pages-of-history",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@10.24.0",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "typecheck": "vue-tsc --noEmit -p tsconfig.json",
    "rename:media": "node ./scripts/rename-media.mjs",
    "test": "vitest run",
    "test:watch": "vitest",
    "preinstall": "only-allow pnpm",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "fix": "node ./scripts/fix.mjs",
    "format": "prettier -w .",
    "format:check": "prettier -c .",
    "check": "pnpm typecheck && pnpm lint && pnpm test",
    "build:dev": "cross-env FAIL_ON_WARNINGS=1 vite build --mode development",
    "precommit:all": "node ./scripts/precommit-all.mjs",
    "prepare": "husky"
  },
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=10.0.0"
  },
  "dependencies": {
    "@babylonjs/core": "^8.39.0",
    "@floating-ui/dom": "^1.7.4",
    "@fontsource/poiret-one": "^5.2.8",
    "@fortawesome/fontawesome-svg-core": "^7.1.0",
    "@fortawesome/free-solid-svg-icons": "^7.1.0",
    "@fortawesome/vue-fontawesome": "^3.1.2",
    "change-case": "^5.4.4",
    "pinia": "^3.0.4",
    "pluralize": "^8.0.0",
    "workbox-window": "^7.1.0",
    "vite-plugin-pwa": "^1.2.0",
    "vue": "^3.5.25",
    "vue-router": "^4.6.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@tailwindcss/postcss": "^4.1.17",
    "@types/node": "^24.10.1",
    "@types/pluralize": "^0.0.33",
    "@vitejs/plugin-vue": "^6.0.2",
    "@vue/eslint-config-typescript": "^14.6.0",
    "autoprefixer": "^10.4.22",
    "cross-env": "^7.0.3",
    "eslint": "^9.39.1",
    "eslint-plugin-vue": "^10.6.2",
    "vue-eslint-parser": "^10.0.0",
    "husky": "^9.1.6",
    "only-allow": "^1.2.2",
    "postcss": "^8.5.6",
    "prettier": "^3.7.3",
    "tailwindcss": "^4.1.17",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.48.0",
    "vite": "^7.2.6",
    "vitest": "^4.0.14",
    "vue-tsc": "^3.1.5"
  }
}
